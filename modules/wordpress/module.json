{
    "name": "wordpress",
    "version": "1.0.0",
    "description": "WordPress compatibility layer for PrestoWorld",
    "type": "bridge",
    "priority": 100,
    "requires": {
        "php": ">=8.1",
        "modules": []
    },
    "providers": [
        "PrestoWorld\\WordPress\\Providers\\WordPressServiceProvider"
    ],
    "autoload": {
        "psr-4": {
            "PrestoWorld\\WordPress\\": "src/"
        }
    },
    "config": {
        "publish": [
            "config/wordpress.php"
        ]
    },
    "enabled": false
}